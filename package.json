{
  "name": "my-component",
  "version": "0.1.0",
  "description": "Componente para teste de publicação npm",
  "author": {
    "name": "Orlando Silva Junior",
    "email": "orlandosj@gmail.com"
  },
  "repository": "https://github.com/orlandosj/testpub",
